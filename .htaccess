RewriteEngine on
RewriteRule ^([a-z,A-Z,0-9]+)/?$ index.php?c=$1 [L,QSA]
RewriteRule ^([a-z,A-Z,0-9]+)/([a-z,A-Z,0-9]+)/?$ index.php?c=$1&a=$2 [L,QSA]
RewriteRule ^([a-z,A-Z,0-9]+)/([a-z,A-Z,0-9]+)/([0-9]+)/?$ index.php?c=$1&a=$2&i=$3 [L,QSA]
RewriteRule ^([a-z,A-Z,0-9]+)/([a-z,A-Z,0-9]+)/([0-9]+)/([0-9]+)/?$ index.php?c=$1&a=$2&i=$3&i2=$4 [L,QSA]